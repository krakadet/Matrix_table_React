{
  "plugins": [
    "react",
    "prettier"
  ],
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  "rules": {
    "no-set-state": "off",
    "strict": 0,
    "jsx-quotes": [
      1,
      "prefer-double"
    ]
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}