{
  "plugins": [
    "react",
    "prettier"
  ],

  "parser": "babel-eslint",

  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier",
     "airbnb"
  ],

  "rules": {
    "no-set-state": "off",
    "strict": 0,
    "jsx-quotes": 1
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}