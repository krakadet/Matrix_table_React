{
  "extends": [
    "react-app",
    "prettier",
     "airbnb",
    "eslint:recommended",
    "plugin:react/recommended"

  ],
  "rules": {
    "jsx-a11y/rule-name": 2,
    "jsx-quotes": [
      1,
      "prefer-double"
    ],
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error"
  },
  "plugins": [
    "prettier",
    "jsx-a11y"
  ],
  "parserOptions": { "ecmaVersion": 9 }

}